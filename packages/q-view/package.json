{
  "name": "q-view",
  "version": "0.0.1",
  "scripts": {
    "dev": "pnpm run start",
    "start": "vite --open",
    "build": "vite build --config vite.config.build.ts && tsc",
    "prepublishOnly": "pnpm run build"
  },
  "type": "module",
  "main": "./lib/index.umd.js",
